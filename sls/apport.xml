<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE settings PUBLIC
 "-//Ubuntu//DTD JobService Service-Level Settings 0.1//EN"
 "http://people.ubuntu.com/~jpeddicord/SLS/0.1/sls.dtd">
<settings>
    <setting name="enable" type="bool">
        <description>Enable crash reporting</description>
        <file>/etc/default/apport</file>
        <parse>enabled=%s</parse>
        <values>
            <value name="true">1</value>
            <value name="false">0</value>
        </values>
    </setting>
</settings>
