<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE settings PUBLIC
 "-//Ubuntu//DTD JobService Service-Level Settings 0.1//EN"
 "http://people.ubuntu.com/~jpeddicord/SLS/0.1/sls.dtd">
<settings>
    <setting name="enabled" type="bool">
        <_description>Enable kernel crash reporting</_description>
        <data>
            <parse file="/etc/default/kerneloops">%n=%s</parse>
        </data>
        <values>
            <value name="true">
                <raw>1</raw>
            </value>
            <value name="false">
                <raw>0</raw>
            </value>
        </values>
    </setting>
</settings>
