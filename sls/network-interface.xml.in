<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE settings PUBLIC
 "-//Ubuntu//DTD JobService Service-Level Settings 0.8//EN"
 "http://people.ubuntu.com/~jpeddicord/SLS/0.8/sls.dtd">
<settings>
    <setting type="label" name="placeholder">
        <description>Instance setting placeholder</description>
    </setting>
    <setting type="bool" name="manual">
        <_description>Manage manually</_description>
        <data>
            <parse get="/usr/lib/jobservice/helpers/network-interface '%j' get">%n %s</parse>
            <parse set="/usr/lib/jobservice/helpers/network-interface '%j' set">%n %s</parse>
        </data>
        <values>
            <value name="true">
                <raw>yes</raw>
            </value>
            <value name="false">
                <raw>no</raw>
            </value>
        </values>
    </setting>
    <setting type="choice" name="netmode">
        <_description>Network mode</_description>
        <data>
            <parse get="/usr/lib/jobservice/helpers/network-interface '%j' get">%n %s</parse>
            <parse set="/usr/lib/jobservice/helpers/network-interface '%j' set">%n %s</parse>
        </data>
        <values>
            <value name="inet_dhcp">
                <_description>DHCP</_description>
                <raw>inet dhcp</raw>
            </value>
            <value name="inet_static">
                <_description>Static</_description>
                <raw>inet static</raw>
            </value>
            <value name="inet6_static">
                <_description>Static (IPv6)</_description>
                <raw>inet6 static</raw>
            </value>
        </values>
    </setting>
    <setting type="str" name="address">
        <_description>IP address</_description>
        <data>
            <parse get="/usr/lib/jobservice/helpers/network-interface '%j' get">%n %s</parse>
            <parse set="/usr/lib/jobservice/helpers/network-interface '%j' set">%n %s</parse>
        </data>
    </setting>
    <setting type="str" name="netmask">
        <_description>Subnet mask</_description>
        <data>
            <parse get="/usr/lib/jobservice/helpers/network-interface '%j' get">%n %s</parse>
            <parse set="/usr/lib/jobservice/helpers/network-interface '%j' set">%n %s</parse>
        </data>
    </setting>
    <setting type="str" name="gateway">
        <_description>Default gateway</_description>
        <data>
            <parse get="/usr/lib/jobservice/helpers/network-interface '%j' get">%n %s</parse>
            <parse set="/usr/lib/jobservice/helpers/network-interface '%j' set">%n %s</parse>
        </data>
    </setting>
</settings>
