<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE settings PUBLIC
 "-//Ubuntu//DTD JobService Service-Level Settings 0.1//EN"
 "http://people.ubuntu.com/~jpeddicord/SLS/0.1/sls.dtd">
<settings>
    <setting type="bool" name="InitiallyPowered">
        <_description>Active on boot</_description>
        <data>
            <parse file="/etc/bluetooth/main.conf">%n = %s</parse>
        </data>
        <values>
            <value name="true">
                <raw>true</raw>
            </value>
            <value name="false">
                <raw>false</raw>
            </value>
        </values>
    </setting>
    <setting type="bool" name="RememberPowered">
        <_description>Remember powered state</_description>
        <data>
            <parse file="/etc/bluetooth/main.conf">%n = %s</parse>
        </data>
        <values>
            <value name="true">
                <raw>true</raw>
            </value>
            <value name="false">
                <raw>false</raw>
            </value>
        </values>
    </setting>
</settings>
